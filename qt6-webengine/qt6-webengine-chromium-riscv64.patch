+      default: break;
+      case '0':	 // 1 string to match.
+        return 27;	 // "s10"
+      case '1':	 // 1 string to match.
+        return 28;	 // "s11"
+      }
+      break;
+    }
+    break;
+  case 4:	 // 9 strings to match.
+    switch (Name[0]) {
+    default: break;
+    case 'f':	 // 8 strings to match.
+      switch (Name[1]) {
+      default: break;
+      case 's':	 // 4 strings to match.
+        if (Name[2] != '1')
+          break;
+        switch (Name[3]) {
+        default: break;
+        case '0':	 // 2 strings to match.
+          return 59;	 // "fs10"
+        case '1':	 // 2 strings to match.
+          return 60;	 // "fs11"
+        }
+        break;
+      case 't':	 // 4 strings to match.
+        if (Name[2] != '1')
+          break;
+        switch (Name[3]) {
+        default: break;
+        case '0':	 // 2 strings to match.
+          return 63;	 // "ft10"
+        case '1':	 // 2 strings to match.
+          return 64;	 // "ft11"
+        }
+        break;
+      }
+      break;
+    case 'z':	 // 1 string to match.
+      if (memcmp(Name.data()+1, "ero", 3) != 0)
+        break;
+      return 1;	 // "zero"
+    }
+    break;
+  }
+  return 0;
