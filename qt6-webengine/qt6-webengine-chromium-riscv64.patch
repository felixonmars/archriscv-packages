+
+static MatchClassKind matchTokenString(StringRef Name) {
+  switch (Name.size()) {
+  default: break;
+  case 1:	 // 2 strings to match.
+    switch (Name[0]) {
+    default: break;
+    case '(':	 // 1 string to match.
+      return MCK__40_;	 // "("
+    case ')':	 // 1 string to match.
+      return MCK__41_;	 // ")"
+    }
+    break;
+  }
+  return InvalidMatchClass;
+}
