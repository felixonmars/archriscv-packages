+  InitMCProcessorInfo(CPU, FS);
+  const FeatureBitset& Bits = getFeatureBits();
+  if (Bits[RISCV::Feature64Bit]) HasRV64 = true;
+  if (Bits[RISCV::FeatureRV32E]) IsRV32E = true;
